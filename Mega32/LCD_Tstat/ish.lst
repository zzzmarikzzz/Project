    81: 0000AC   E0B0  ldi		XH, HIGH(4*(60))	; Должна быть пауза между таймслотами, вообще-то от 1 мкс, но здесь сделали 60 мкс
    82: 0000AD   EFA0  ldi		XL, LOW(4*(60))
    83: 0000AE   DFD6  rcall	Wait4xCycles
    84: 0000AF   9508  ret
