    62: ; ОТПРАВКА 1 БИТА
    63: ; Эта процедура отправляет 1 бит в линию 1-Wire.
    64: ; Отправляемый бит должен быть помещен в флаг С статусного регистра
    65: ;------------------------------------------------------------------------------
    66: ; Input : C - bit to write
    67: ;------------------------------------------------------------------------------
    68: OWWriteBit:
    69: 0000A3   F418  brcc	OWWriteZero			; Если флаг С = 0, то переход на OWWriteZero
    70: 0000A4   E0B0  ldi		XH, HIGH(4*(1))	; Для посылки 1 линию нужно придавить в 0 всего на 1 мкс
    71: 0000A5   E0A4  ldi		XL, LOW(4*(1))
    72: 0000A6   C002  rjmp	OWWriteOne			; переходим к отправке
    73: OWWriteZero:	
    74: 0000A7   E0B1  ldi		XH, HIGH(4*(120))	; Для посылки 0 линию нужно придавить в 0 на 120 мкс
    75: 0000A8   EEA0  ldi		XL, LOW(4*(120))
    76: OWWriteOne:
    77: 0000A9   9AD0  sbi		OW_DDR, OW_DQ		; Переводим порт на выход, а там уже был 0, соответственно и линия придавливается в 0
    78: 0000AA   DFDA  rcall	Wait4xCycles		; ждем
    79: 0000AB   98D0  cbi		OW_DDR, OW_DQ		; Переводим порт на вход
