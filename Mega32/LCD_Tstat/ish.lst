    29: ; СБРОС
    30: ; Каждая передача по протоколу 1-Wire начинается с сигнала Reset.
    31: ; После вызова этой процедуры в флаге Т регистра SGER содержится бит 
    32: ; присутствия: 1 - если на шине нет устройств, 0 - если есть
    33: ;------------------------------------------------------------------------------
    34: ; Output : T - presence bit
    35: ;------------------------------------------------------------------------------
    36: OWReset:
    37: 000093   98D8  cbi		OW_PORT,OW_DQ		; Выводим в порт 0
    38: 000094   9AD0  sbi		OW_DDR,OW_DQ		; Переключаем порт на вывод
    39: 
    40: 000095   E0B7  ldi		XH, HIGH(4*(500))	; Ждем 470 микросекунд при придавленной в 0 линии. Это и есть импульс сброса.
    41: 000096   EDA0  ldi		XL, LOW(4*(500))
    42: 000097   DFED  rcall		Wait4xCycles
    43: 	
    44: 000098   98D0  cbi		OW_DDR,OW_DQ		; Переключаем порт на ввод
    45: 
    46: 000099   E0B1  ldi		XH, HIGH(4*(70))	; выжидаем 70 мкс (необходимое минимальное время реакции устройств на сброс)
    47: 00009A   E1A8  ldi		XL, LOW(4*(70))
    48: 00009B   DFE9  rcall		Wait4xCycles
    49: 
    50: 00009C   9468  set							; Устанавливаем флаг Т в 1
    51: 00009D   9BC8  sbis	OW_PIN,OW_DQ		; Если на линии после паузы осталась 1, значит устройств 1-Wire на ней нет. Пропускаем след. команду
    52: 00009E   94E8  clt							; Линия была в 0 - значит на ней кто-то есть, и ответил нам импульсом PRESENCE
    53: 
    54: 00009F   E0B3  ldi		XH, HIGH(4*(240))	; Пауза 240 мкс после сброса
    55: 0000A0   ECA0  ldi		XL, LOW(4*(240))
    56: 0000A1   DFE3  rcall		Wait4xCycles
    57: 
    58: 0000A2   9508  ret
    59: 
