    62: ; ОТПРАВКА 1 БИТА
    63: ; Эта процедура отправляет 1 бит в линию 1-Wire.
    64: ; Отправляемый бит должен быть помещен в флаг С статусного регистра
    65: ;------------------------------------------------------------------------------
    66: ; Input : C - bit to write
    67: ;------------------------------------------------------------------------------
    68: OWWriteBit:
    69: 00004E   F418  brcc	OWWriteZero			; Если флаг С = 0, то переход на OWWriteZero
    70: 00004F   E0B0  ldi		XH, HIGH(4*(1))	; Для посылки 1 линию нужно придавить в 0 всего на 1 мкс
    71: 000050   E0A4  ldi		XL, LOW(4*(1))
    72: 000051   C002  rjmp	OWWriteOne			; переходим к отправке
    73: OWWriteZero:	
    74: 000052   E0B1  ldi		XH, HIGH(4*(120))	; Для посылки 0 линию нужно придавить в 0 на 120 мкс
    75: 000053   EEA0  ldi		XL, LOW(4*(120))
    76: OWWriteOne:
    77: 000054   9AD0  sbi		OW_DDR, OW_DQ		; Переводим порт на выход, а там уже был 0, соответственно и линия придавливается в 0
    78: 000055   DFDA  rcall	Wait4xCycles		; ждем
    79: 000056   98D0  cbi		OW_DDR, OW_DQ		; Переводим порт на вход
    80: 	
    81: 000057   E0B0  ldi		XH, HIGH(4*(60))	; Должна быть пауза между таймслотами, вообще-то от 1 мкс, но здесь сделали 60 мкс
    82: 000058   EFA0  ldi		XL, LOW(4*(60))
    83: 000059   DFD6  rcall	Wait4xCycles
    84: 00005A   9508  ret
