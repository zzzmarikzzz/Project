    28: ; СБРОС
    29: ; Каждая передача по протоколу 1-Wire начинается с сигнала Reset.
    30: ; После вызова этой процедуры в флаге Т регистра SGER содержится бит 
    31: ; присутствия: 1 - если на шине нет устройств, 0 - если есть
    32: ;------------------------------------------------------------------------------
    33: ; Output : T - presence bit
    34: ;------------------------------------------------------------------------------
    35: OWReset:
    36: 0000B0   9894  cbi		OW_PORT,OW_DQ		; Выводим в порт 0
    37: 0000B1   9A8C  sbi		OW_DDR,OW_DQ		; Переключаем порт на вывод
    38: 
    39: 0000B2   E0B9  ldi		XH, HIGH(C4PUS*470)	; Ждем 470 микросекунд при придавленной в 0 линии. Это и есть импульс сброса.
    40: 0000B3   E2AE  ldi		XL, LOW(C4PUS*470)
    41: 0000B4   DFED  rcall		Wait4xCycles
    42: 	
    43: 0000B5   988C  cbi		OW_DDR,OW_DQ		; Переключаем порт на ввод
    44: 
    45: 0000B6   E0B1  ldi		XH, HIGH(C4PUS*70)	; выжидаем 70 мкс (необходимое минимальное время реакции устройств на сброс)
    46: 0000B7   E5AE  ldi		XL, LOW(C4PUS*70)
    47: 0000B8   DFE9  rcall		Wait4xCycles
    48: 
    49: 0000B9   9468  set							; Устанавливаем флаг Т в 1
    50: 0000BA   9B84  sbis	OW_PIN,OW_DQ		; Если на линии после паузы осталась 1, значит устройств 1-Wire на ней нет. Пропускаем след. команду
    51: 0000BB   94E8  clt							; Линия была в 0 - значит на ней кто-то есть, и ответил нам импульсом PRESENCE
    52: 
    53: 0000BC   E0B4  ldi		XH, HIGH(C4PUS*240)	; Пауза 240 мкс после сброса
    54: 0000BD   EBA0  ldi		XL, LOW(C4PUS*240)
    55: 0000BE   DFE3  rcall		Wait4xCycles
    56: 
    57: 0000BF   9508  ret
