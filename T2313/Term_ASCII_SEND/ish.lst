    61: ; ОТПРАВКА 1 БИТА
    62: ; Эта процедура отправляет 1 бит в линию 1-Wire.
    63: ; Отправляемый бит должен быть помещен в флаг С статусного регистра
    64: ;------------------------------------------------------------------------------
    65: ; Input : C - bit to write
    66: ;------------------------------------------------------------------------------
    67: OWWriteBit:
    68: 0000C0   F418  brcc	OWWriteZero			; Если флаг С = 0, то переход на OWWriteZero
    69: 0000C1   E0B0  ldi		XH, HIGH(C4PUS*1)	; Для посылки 1 линию нужно придавить в 0 всего на 1 мкс
    70: 0000C2   E0A5  ldi		XL, LOW(C4PUS*1)
    71: 0000C3   C002  rjmp	OWWriteOne			; переходим к отправке
    72: OWWriteZero:	
    73: 0000C4   E0B2  ldi		XH, HIGH(C4PUS*120)	; Для посылки 0 линию нужно придавить в 0 на 120 мкс
    74: 0000C5   E5A8  ldi		XL, LOW(C4PUS*120)
    75: OWWriteOne:
    76: 0000C6   9A8C  sbi		OW_DDR, OW_DQ		; Переводим порт на выход, а там уже был 0, соответственно и линия придавливается в 0
    77: 0000C7   DFDA  rcall	Wait4xCycles		; ждем
    78: 0000C8   988C  cbi		OW_DDR, OW_DQ		; Переводим порт на вход
    79: 	
    80: 0000C9   E0B1  ldi		XH, HIGH(C4PUS*60)	; Должна быть пауза между таймслотами, вообще-то от 1 мкс, но здесь сделали 60 мкс
    81: 0000CA   E2AC  ldi		XL, LOW(C4PUS*60)
    82: 0000CB   DFD6  rcall	Wait4xCycles
    83: 0000CC   9508  ret
